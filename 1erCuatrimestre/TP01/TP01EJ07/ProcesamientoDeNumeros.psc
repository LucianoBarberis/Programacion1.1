Algoritmo ProcesamientoDeNumeros
	Definir num1, num2, sum, prod, res Como Entero;
	
	Escribir "Ingrese el primer valor";
	Leer num1;
	Escribir "Ingrese el segundo valor";
	Leer num2;
	
	sum <- num1 + num2;
	prod <- num1 * num2;
	res <- num1 - num2;
	
	Escribir "Suma: ", sum
	Escribir "Producto: ", prod
	Escribir "Resta: ", res;
FinAlgoritmo
